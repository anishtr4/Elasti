# Environment Variables

# CHAT Provider (fast inference: groq, gemini, openai)
CHAT_PROVIDER=groq

# EMBEDDING Provider (must support embeddings: gemini, openai)
# Note: Groq does not currently support embeddings.
EMBEDDING_PROVIDER=gemini

# API Keys (only the one matching LLM_PROVIDER is required)
GEMINI_API_KEY=your_gemini_api_key_here
GROQ_API_KEY=gsk_your_groq_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Infrastructure
OPENSEARCH_URL=http://localhost:9200
REDIS_URL=redis://localhost:6380
